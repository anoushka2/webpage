        "/supported_language/{identifier}/ie": {
            "post": {
                "tags": [
                    "run"
                ],
                "summary": "ie result in json format for input text document",
                "description": "Receives ie result in json format for input text document.",
                "operationId": "controllers.default_controller.supported_language_identifier_ie_post",
                "consumes": [
                    "multipart/form-data"
                ],
                "parameters": [
                    {
                        "name": "identifier",
                        "in": "path",
                        "description": "language code identifier for the language used for ie. (e.g. \"hau\", \"tur\", \"yor\" and etc.)",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "text",
                        "in": "formData",
                        "description": "text document used as input to ie system.",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "JMIST_elisa_xml",
                        "in": "formData",
                        "description": "Upload JMIST ELISA parallel xml file.",
                        "required": false,
                        "type": "file"
                    },
                    {
                        "name": "model",
                        "in": "query",
                        "description": "Receives ie result in json format for the input text document. One ie model is provided:  (1) litesaber-svml: SVM lattice approach",
                        "required": true,
                        "type": "array",
                        "items": {
                            "type": "string",
                            "default": "litesaber-svml",
                            "enum": [
                                "litesaber-svml"
                            ]
                        }
                    },
                    {
                        "name": "model_id",
                        "in": "query",
                        "description": "(optional) If a model id is entered, name tagger will return results using the specific model. Use train function below to train a model.",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "output_format",
                        "in": "query",
                        "description": "Choose result name tagging result format: (1)KnowledgeGraph: JSON knowledge graph format. (2) EvalTab: NIST LORELEI evaluation format.",
                        "required": true,
                        "type": "array",
                        "items": {
                            "type": "string",
                            "default": "KnowledgeGraph",
                            "enum": [
                                "KnowledgeGraph",
                                "EvalTab"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "JSON format for entities, events and relations in knowledge graph.",
                        "schema": {
                            "$ref": "#/definitions/KnowledgeGraph"
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                },
                "x-tags": [
                    {
                        "tag": "run"
                    }
                ]
            }
        },
