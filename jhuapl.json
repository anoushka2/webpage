    "definitions": {
        "KnowledgeGraph": {
            "type": "object",
            "properties": {
                "attribute": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Attribute"
                    }
                },
                "entity_mention": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/EntityMention"
                    }
                },
                "entity": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Entity"
                    }
                },
                "event_mention": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/EventMention"
                    }
                },
                "event": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Event"
                    }
                },
                "relation": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Relation"
                    }
                }
            }
        },
        "Attribute": {
            "type": "object",
            "required": [
                "attribute_head",
                "attribute_id",
                "attribute_type",
                "attribute_value",
                "docid:offset"
            ],
            "properties": {
                "attribute_id": {
                    "type": "string"
                },
                "attribute_type": {
                    "type": "string"
                },
                "docid:offset": {
                    "type": "string"
                },
                "attribute_head": {
                    "type": "string"
                },
                "attribute_value": {
                    "type": "string"
                }
            }
        },
        "EntityMention": {
            "type": "object",
            "required": [
                "docid:offsets",
                "mention_head",
                "mention_id",
                "mention_type",
                "sentenceid:tokenindexes",
                "translations",
                "transliterations"
            ],
            "properties": {
                "mention_id": {
                    "type": "string"
                },
                "docid:offsets": {
                    "type": "string"
                },
                "sentenceid:tokenindexes": {
                    "type": "string"
                },
                "mention_head": {
                    "type": "string"
                },
                "mention_type": {
                    "type": "string"
                },
                "translations": {
                    "type": "array",
                    "description": "a list of English translations of the entity mention",
                    "items": {
                        "type": "array",
                        "description": "array of size 2, first element is the translation and the second is confidence score",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "transliterations": {
                    "type": "array",
                    "description": "a list of English transliterations of the entity mention",
